{
  "name": "bhashish-language",
  "displayName": "BHASHISH Language",
  "description": "Syntax highlighting for the BHASHISH Hinglish programming language",
  "version": "0.0.1",
  "publisher": "bhashish",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "bhashish",
        "aliases": ["BHASHISH", "bhashish"],
        "extensions": [".hl"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "bhashish",
        "scopeName": "source.bhashish",
        "path": "./syntaxes/bhashish.tmLanguage.json"
      }
    ]
  }
}
